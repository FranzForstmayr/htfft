CAPI=2:

name : ::htfft_example
description: An example using the HTFFT generator.

filesets:
  default:
    depend:
      - htfft_pkg
      - butterfly
      - memory
      - initial_memory
      - final_memory
      - htfft_gen

targets:
  default:
    generate : [htfft_gen]
    toplevel : htfft_8_example
    filesets: [default]

generate:
  htfft_gen:
    generator : htfft_gen
    parameters:
      suffix: _8_example
      n: 4
      size: 2
      input_width: 32
      twiddle_width: 32
