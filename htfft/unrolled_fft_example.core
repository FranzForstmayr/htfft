CAPI=2:

name : ::unrolled_fft_example
description: An example using the unrolled FFT generator.

filesets:
  default:
    depend:
      - butterfly
      - comb_reordering
      - unrolled_fft

targets:
  default:
    generate : [unrolled_fft_gen]
    toplevel : unrolled_fft_64_example
    filesets: [default]

generate:
  unrolled_fft_gen:
    generator : unrolled_fft_gen
    parameters:
      suffix: _example
      size: 64
      input_width: 16
      twiddle_width: 16
